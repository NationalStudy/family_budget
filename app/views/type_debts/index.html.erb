<p id="notice"><%= notice %></p>

<h1>Listing Type Debts</h1>

<table>
  <thead>
    <tr>
      <th>Type debt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @type_debts.each do |type_debt| %>
      <tr>
        <td><%= type_debt.type_debt %></td>
        <td><%= link_to 'Show', type_debt %></td>
        <td><%= link_to 'Edit', edit_type_debt_path(type_debt) %></td>
        <td><%= link_to 'Destroy', type_debt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Type debt', new_type_debt_path %>
